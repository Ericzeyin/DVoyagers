              }

           var infowindow = new google.maps.InfoWindow({
               content: "",
               maxWidth: 400
           });



           bounds = new google.maps.LatLngBounds();

           for (var i = 0; i < markersArray.length; i++) {
               markersArray[i].setMap(map);

               loc = new google.maps.LatLng(markersArray[i].position.lat(), markersArray[i].position.lng());
               bounds.extend(loc);

               google.maps.event.addListener(markersArray[i], 'click', function (event) {
                   infowindow.setContent(this.content);
                   infowindow.open(map, this);
               });
           }

           var markerCluster = new MarkerClusterer(map, markersArray, { imagePath: '../m' });

           map.fitBounds(bounds);
           map.panToBounds(bounds);

        }
